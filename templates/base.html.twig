						</svg>
						<!--end::Svg Icon-->
					</span>
				</button>
				<!--end::Topbar Mobile Toggle-->
			</div>
			<!--end::Toolbar-->
		</div>
		<!--end::Header Mobile-->
		<div
			class="d-flex flex-column flex-root">
			<!--begin::Page-->
			<div
				class="d-flex flex-row flex-column-fluid page">
				<!--begin::Aside-->
				<div
					class="aside aside-left aside-fixed d-flex flex-column flex-row-auto" id="kt_aside">
					<!--begin::Brand-->
					<div
						class="brand flex-column-auto " id="kt_brand">
						<!--begin::Logo-->
						<a href="#" class="brand-logo">
							<img alt="Logo" class="pull-right" src="{{ asset('files/site_setting/'~site_logo ) }}" width="50" height='50'/>
						</a>
						<!--end::Logo-->
						<!--begin::Toggle-->
						<button class="brand-toggle btn btn-sm px-0" id="kt_aside_toggle">
							<span
								class="svg-icon svg-icon svg-icon-xl">
