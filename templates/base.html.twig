									<span class="menu-text">

										<h5
											class="text-default mt-2">
											{# <a href="#" class="brand-logo">
																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																											<img alt="Logo" class="pull-right" src="{{ asset('files/site_setting/'~site_logo ) }}" width="50" height='50'/>
																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																																										</a> #}
											<b>{{ app_description |trans }}</b>
										</h5>
									</span>
									<!--end::Header Nav-->
								</div>
								<!--end::Header Menu-->
							</div>
							<!--end::Header Menu Wrapper-->
