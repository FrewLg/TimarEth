								</div>

								<div class="dropdown">
									<div class="topbar-item ml-2" data-toggle="dropdown" data-offset="10px,0px">
										<div class="btn btn-icon w-auto btn-clean d-flex align-items-center btn-lg px-2" id="kt_quick_user_toggle">
											<i class="fa fa-language text-success ml-4 mr-1"></i>

											<span class="text-capitalize font-weight-bolder   mr-4">
												{{ app.request.getLocale() }}
											</span>
										</div>
									</div>
									<div class="dropdown-menu p-0 m-0 dropdown-menu-anim-up dropdown-menu-sm dropdown-menu-right ">
										{% include "locale.html.twig" %}
									</div>
								</div>
								<div class="dropdown">
									<div class="topbar-item ml-2" data-toggle="dropdown" data-offset="10px,0px">
										<div class="btn btn-icon w-auto btn-clean d-flex align-items-center btn-lg px-2" id="kt_quick_user_toggle">
											<span class="text-dark-50 font-weight-bolder font-size-base d-none d-md-inline mr-3">

												{% if app.user.profile  and app.user.profile.image %}
													<img src="{{ asset(profile_pictures~'/'~app.user.profile.image ) }}" alt="profile-user" class="rounded-circle thumb-xs" style="width:30px;height: 30px;"/>
												{% elseif app.user  %}
													<img src="{{ asset('defaultuser.png') }}" alt="profile-user" class="rounded-circle thumb-xs" style="width:30px;height: 30px;"/>
												{% endif %}

												{% if app.user %}
													{{ app.user }}
												{% endif %}
												{# {{app.user.profile.image}} #}
											</span>
											<span class="symbol symbol-35 symbol-light-success">
												<span class="symbol-label font-size-h5 font-weight-bold text-uppercase">
													{% if app.user %}
														{{ app.user.email|striptags|slice(0, 1)|raw }}
													{% endif %}
												</span>
											</span>
										</div>
									</div>
									<div class="dropdown-menu p-0 m-0 dropdown-menu-anim-up dropdown-menu-sm dropdown-menu-right ">

										<ul
											class="navi navi-hover py-4  ">
											<!--begin::Item-->

											<li class="navi-item">
												<a href="{{ path('my_profile')}}" class="navi-link">
													<span class="symbol symbol-20 mr-3">

														<i class="fa fa-user "></i>
													</span>
													<span class="navi-text">{% trans %}Profile{% endtrans %}</span>
												</a>
											</li>

											<li class="navi-item">
												<a href="{{ path('app_logout')}}" class="navi-link">
													<span class="symbol symbol-20 mr-3">

														<i class="fa fa-signout "></i>
													</span>
													<span class="navi-text">{% trans %}Logout{% endtrans %}</span>
												</a>
											</li>

										</ul>
									</div>
								</div>
								{% if is_granted("ROLE_USER")%}{% endif %}

								<!--end::User-->
							</div>
							<!--end::Topbar-->
						</div>
						<!--end::Container-->
					</div>

					<div
						id="kt_content" class="content d-flex flex-column flex-column-fluid">
						<!--begin::Subheader-->
						<div class=" subheader py-2 py-lg-4 subheader-solid " style="margin-top:-60px;">
							<div
								class="container-fluid d-flex align-items-center justify-content-between flex-wrap flex-sm-nowrap">
								<!--begin::Info-->
								<div
									class="d-flex align-items-center flex-wrap mr-1">
									<!--begin::Page Heading-->
									<div
										class="d-flex align-items-baseline mr-5">
										<!--begin::Page Title-->
										<h5 class="text-dark font-weight-bold  mr-5">{% trans %}Home{% endtrans %}</h5>
										<!--end::Page Title-->
										<!--begin::Breadcrumb-->
										<ul class="breadcrumb breadcrumb-transparent breadcrumb-dot font-weight-bold font-size-sm">
											<li class="breadcrumb-item">
												<a href="" class="text-muted"> {% block subheader %}{% endblock  %}
													</a>
												</li>
												<li class="breadcrumb-item">
													<a href="" class="text-muted"> {% block pagetitle %}{% endblock  %}
														</a>
													</li>
												</ul>
												<!--end::Breadcrumb-->
											</div>
											<!--end::Page Heading-->
										</div>
									</div>
								</div>

								<div class="d-flex flex-column-fluid">
									<div class="container">
										{% include "flash_messages.html.twig" %}

										{% block body %}{# {{ file_url(item.content['#file'].getFileUri()) }} #}
											{# {% set url = file_url(content.field_pdf_file|render|striptags|trim) %} #}
										{% endblock  %}
									</div>
								</div>
							</div>

							<div
								class="footer bg-white py-4 d-flex flex-lg-column" id="kt_footer">
								<!--begin::Container-->
								<div
									class="container-fluid d-flex flex-column flex-md-row align-items-center justify-content-between">
									<!--begin::Copyright-->
									<div class="text-dark order-2 order-md-1">
										<span class="text-muted font-weight-bold mr-2">
											&copy;
											{{copyright}}
											<script>
												document.write(new Date().getFullYear())
											</script>
										</span>
										<a href="{{app_url}}" target="_blank" class="text-dark-75 text-hover-primary">{{acronym}}</a>
									</div>
									<!--end::Copyright-->
									<!--begin::Nav-->
									<div class="nav nav-dark">
										<a href="http://sero.ephi.gov.et" target="_blank" class="nav-link pl-0 pr-5">{% trans %}About{% endtrans %}
											{{ acronym }}
											|</a>
										<a href="http://ephi.gov.et" target="_blank" class="nav-link pl-0 pr-5">{% trans %}EPHI{% endtrans %}
											|</a>
										<a href="{{app_url}}" target="_blank" class="nav-link pl-0 pr-0">{% trans %}Contact{% endtrans %}
											|</a>
										<a href="{{path('compliant')}}" target="_blank" class="nav-link pl-0 pr-0 m-1">Submit a Compliant</a>

									</div>
									<!--end::Nav-->
								</div>
								<!--end::Container-->
							</div>
							<!--end::Footer-->
						</div>
					</div>
				</div>
