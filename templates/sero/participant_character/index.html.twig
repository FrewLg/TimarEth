{% extends 'base.html.twig' %}

{% block title %}ParticipantCharacter index{% endblock %}

{% block body %}
    <h1>ParticipantCharacter index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Name</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for participant_character in participant_characters %}
            <tr>
                <td>{{ participant_character.id }}</td>
                <td>{{ participant_character.name }}</td>
                <td>
                    <a href="{{ path('app_s_e_r_o_participant_character_show', {'id': participant_character.id}) }}">show</a>
                    <a href="{{ path('app_s_e_r_o_participant_character_edit', {'id': participant_character.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_s_e_r_o_participant_character_new') }}">Create new</a>
{% endblock %}
