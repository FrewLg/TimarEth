{% extends 'base.html.twig' %}

{% block title %}Version{% endblock %}

{% block body %}
    <h1>Version</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ version.id }}</td>
            </tr>
            <tr>
                <th>Date</th>
                <td>{{ version.date ? version.date|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>ChangesMade</th>
                <td>{{ version.changesMade }}</td>
            </tr>
            <tr>
                <th>VersionNumber</th>
                <td>{{ version.versionNumber }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ version.createdAt ? version.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>Attachment</th>
                <td>{{ version.attachment }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_s_e_r_o_version_index') }}">back to list</a>

    <a href="{{ path('app_s_e_r_o_version_edit', {'id': version.id}) }}">edit</a>

    {{ include('sero/version/_delete_form.html.twig') }}
{% endblock %}
